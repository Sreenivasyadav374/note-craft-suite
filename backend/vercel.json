{
  "version": 2,
  "name": "note-craft-backend-api",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/",
      "dest": "dist/index.js"
    }
  ],
  "buildCommand": "npm run build"
}
